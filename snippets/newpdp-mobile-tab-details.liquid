    {%if product.metafields.Stuhrling.StyleImage != blank and product.metafields.Stuhrling.style_content != blank %}
  <div class="Style-product">
            <div class="container-fluid">
              <div class="row rows_t_row">

                <div class="col-md-6 col-sm-6 col-xs-12">
                  <div class="images"><img class="lazyload" src="data:image/gif;base64,R0lGODlhAQABAIAAAP///wAAACH5BAEAAAAALAAAAAABAAEAAAICRAEAOw==" data-src="{{ product.metafields.Stuhrling.StyleImage }}"></div>
                </div>
                <div class="col-md-6 col-sm-6 col-xs-12">
                  <div class="content-section">
                   {%if product.metafields.Stuhrling.style_title != blank %}
                    <h2>{{ product.metafields.Stuhrling.style_title }}</h2>
                    {%endif%}
                    <p class="content_para">    {{ product.metafields.Stuhrling.style_content }}</p>
                  </div>
                </div>
              </div>
            </div>
          </div>
{%endif%}
