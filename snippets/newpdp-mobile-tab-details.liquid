     {%if product.metafields.Stuhrling.ProtectionImage != blank and product.metafields.Stuhrling.ProtectionText != blank %}

<div class="Protection-product">
  <div class="container-fluid">
  <div class="row">
  <div class="col-md-6 col-sm-6 col-xs-12">
     <div class="content-section">
       
         {%if product.metafields.Stuhrling.Protection_title != blank %}
                    <h2>{{ product.metafields.Stuhrling.Protection_title }}</h2>
                    {%endif%}
       <p class="content_para">      {{ product.metafields.Stuhrling.ProtectionText }}</p>
     </div>
  </div>
  <div class="col-md-6 col-sm-6 col-xs-12">
    <div class="images third-image"><img class="lazyload" src="data:image/gif;base64,R0lGODlhAQABAIAAAP///wAAACH5BAEAAAAALAAAAAABAAEAAAICRAEAOw==" data-src="{{ product.metafields.Stuhrling.ProtectionImage }}"></div>
