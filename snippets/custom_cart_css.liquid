/* --------------------------------------------cart ---------------------------------------- */
.mini_cart {
  max-width: 450px;
  background: #ffffff;
  position: fixed;
  top: 0;
  right: 0;
  z-index: 11111111;
  border: 1px solid;
  width: 100%;
  transition: 1s all;
  height: 100%;
  transform: translate(0px);
}
.extras {
  transition: 1s all;
  transform: translate(460px);
}
div.mini_cart form {
  margin-bottom: 15px;
}
.bottom_buttons {
  overflow: hidden;
  padding: 0;
  bottom: 0;
  position: absolute;
  width: 100%;
  background-color: #fff;
  display: block;
  box-shadow: 0 -0.3125rem 0.3125rem -0.3125rem #c7c7c7;
  z-index: 99;
}
