@media (min-width: 2000px) {
  div.mini_cart .cart_middle_info {
    height: auto;
  }
}
@media (max-width: 749px) {
  .bottom_buttons .cart__subtotal-price {
    float: right;
    margin: 0px;
  }
  .bottom_buttons .cart__subtotal {
    display: block;
    width: 100%;
    clear: both;
    float: left;
  }
  div.Add_plan > a.plan_btn {
    font-size: 10px;
  }
}
@media only screen and (max-width: 480px) {
  .mini_cart {
    max-width: 380px;
  }
  
  div.cart_price_btn {
    position: fixed;
    bottom: 5px;
    width: 100%;
    left: 0;
    z-index: 9;
    margin: 0 auto;
  }
  .cart_price_btn .money {
    display: block;
  }
  .cart_price_btn button {
    text-shadow: 1px 1px 0px #6e0707;
    -webkit-box-shadow: inset 1px 2px 3px 0px #a75111, 0px 5px 3px 0px #da5b00, 0px 10px 5px #444;
    -moz-box-shadow: inset 1px 2px 3px 0px #a75111, 0px 5px 3px 0px #da5b00, 0px 10px 5px #444;
    box-shadow: inset 1px 2px 3px 0px #a75111, 0px 5px 3px 0px #da5b00, 0px 10px 5px #444;
    border: none;
    width: 100%;
    padding: 16px 20px;
    display: flex;
    align-items: center;
    justify-content: space-between;
    color: #fff;
    font-size: 16px !important;
    text-transform: uppercase;
    font-weight: 600;
    margin: 0 auto !important;
  }
  div.cart_price_btn {
    display: block;
  }
  .product-recommendations .spf-product-card__price-wrapper {
    flex-direction: column;
  }
  .image_cart {
    height: auto;
  }
  #af_cart_extra {
    margin: 0px !important;
  }
}
@media only screen and (max-width: 400px) {
  .mini_cart {
    max-width: 360px;
  }
}
@media only screen and (max-width: 380px) {
  .mini_cart {
    max-width: 340px;
  }
}

