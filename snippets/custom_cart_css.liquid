}
.plus-minus-update {
  display: block;
  position: absolute;
  right: 10px;
  top: 50%;
  transform: translate(0px, -50%);
}
.plus-minus-update span {
  color: #000;
  display: block;
  padding: 0px 3px;
  background: #80808026;
  margin: 2px 0px;
  line-height: 0;
}
.plus-minus-update {
