                    <div class="cart_addition" data-id="{{ product.variants.first.id }}">
                      <span class="add-btn1">+ Add to Cart</span>
                      <span class="added_block" style="display:none;"><i class="fa fa-check" aria-hidden="true"></i>
                        Added to Cart</span>
                    </div>
                  </div>
                </div>
              </div>
              {% endfor %}
            </div>
          </div>
          {% endcomment %}
        </div>
