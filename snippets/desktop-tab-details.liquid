                        {% if product.metafields.Stuhrling.Banner_SubHeading_2 != blank %}
                        {{ product.metafields.Stuhrling.Banner_SubHeading_2 }}
                        {% endif %}
                      </div>


                      <hr class="product-banner-border-line close_quotes close--{{ forloop.index }}"/>

                    </div>
                    {% endif %}
                  </div>



                </div>
              </div>
              {% if product.metafields.Stuhrling.Banner_Image_Link_2  != blank %}
            </a>
            {% endif %}
          </div>
          </div>
          {% endif %}
<!--          Reviews with Banner - End  -->
