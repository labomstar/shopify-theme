{{ 'advanced_product_design.css' | asset_url | stylesheet_tag }}
<link href="https://fonts.googleapis.com/css2?family=Kaushan+Script&display=swap" rel="stylesheet">
<style>
    form#justuno_form {
      display: none;
  }
    div.product_desc1
    {
      background-image:url({%if product.metafields.Stuhrling.Product_Gallery_BG_Image %}{{ product.metafields.Stuhrling.Product_Gallery_BG_Image }}{% else %}https://cdn.shopify.com/s/files/1/0333/0936/0264/files/Product_desc_back1.jpg{% endif %});
    }
    div.product_desc2
    {
      background-image:url({%if product.metafields.Stuhrling.Product_Gallery_BG_Image2 %}{{ product.metafields.Stuhrling.Product_Gallery_BG_Image2 }}{% else %}https://cdn.shopify.com/s/files/1/0333/0936/0264/files/Product_desc_back2.jpg{% endif %});
    }
    div.product_desc1.product_gallery_3
    {
      background-image:url({%if product.metafields.Stuhrling.Product_Gallery3_BG_Image %}{{ product.metafields.Stuhrling.Product_Gallery3_BG_Image }}{% else %}https://cdn.shopify.com/s/files/1/0333/0936/0264/files/Product_desc_back1.jpg{% endif %});
    }
    {% for variant in product.variants %}
    {% assign current = product.selected_or_first_available_variant %}
    {%if current.id == variant.id %}
    @media(min-width:768px)
    {
      .product_outer_background.advanced_product_design
      {
        background-color:#000;
        background-image:url({{ variant.metafields.Stuhrling.Product_Main_BG_Banner }});
        background-repeat: no-repeat;
      }
      .medium--hide--title {
        display: none;
      }
    }
    @media(max-width:767px)
    {
      .product_outer_background.advanced_product_design
      {
        background-color:#000;
        background-image:url({% if variant.metafields.Stuhrling.Product_Main_Mobile_BG_Banner %}{{ variant.metafields.Stuhrling.Product_Main_Mobile_BG_Banner }}{% else %}{{ variant.metafields.Stuhrling.Product_Main_BG_Banner }}{% endif %});
        background-size: cover;
        background-repeat: no-repeat;

      }
      div.product-head {
        padding-top: 160.18px;
      }
      .mobile_heading {
        color: #fff;
        font-size: 20px;
        padding-top: 20px;
        margin-top: 0px;
        text-align: center;
      }


      .quant-buy-contain div.cart_price_btn
      {
        display:block;
        position: fixed;
        bottom: 0px;
        width: 100%;
        left: 0;
        z-index: 9;
        margin: 0 auto;
        }


    }
    {% endif %}
    {% endfor %}

    @media(max-width:767px)
    {
      div#shopify-section-product-advanced-template div.product_desc1
      {
        background-image:url({{ product.metafields.Sturhling.Product_gallery_mobileBg }});
      }
      div#shopify-section-product-advanced-template div.product_desc2
      {
        background-image:url({{ product.metafields.Sturhling.Product_gallery2_mobileBg }});
        max-height: 450px;
      }
      div#shopify-section-product-advanced-template div.product_desc1.product_gallery_3
      {
        background-image:url({{ product.metafields.Sturhling.Product_gallery3_mobileBg }});
      }

    }
     @media only screen and (min-width: 481px) and (max-width: 768px)
       {
       div#shopify-section-product-advanced-template div.product_outer_background.advanced_product_design{
         min-height: 1200px;
         max-height: 1200px;
       }
         div.product_video_cont.mobile>div.video_thumb {
           min-height: 400px;
         }
         div.product_desc1 {
           background-position: top center;
         }
         div#shopify-section-product-advanced-template div.product_desc1 div.desc-wrap.desc2
         {
           padding: 230px 0 60px 0;
         }
         div#shopify-section-product-advanced-template div.product_desc1 div.desc-wrap.desc1 {
           padding: 330px 0 50px 0;
         }

         div#shopify-section-product-advanced-template div.product_desc2 img.mobile_img {
           display: block;
           width: 100%;
           margin: 0 auto;
           text-align: center;
         }
       }
    @media (max-width:375px)
      {
        div.product-head>div.product-content-sec div.swatch {
          padding-top: 0px;
        }
        .swatch .swatch-element {
          max-width: 70px;
          height: 70px;
        }
        .msrp_mobile_price .product--value
        {
          font-size:14px !important;
        }
        div.product-head>div.product-content-sec .price_part, .price_part #ComparePrice
        {
          font-size:20px !Important;

        }
      }
    @media(max-width:360px)
      {
        div.product-head {
          padding-top: 140.18px;
        }
      }
</style>

<!-- Product Details with Background Variant Image - Start -->
<div class="product_outer_background advanced_product_design">
  <div class="wrapper-mini">
    <div
      itemscope
      itemtype="http://schema.org/Product"
      class="product_top_part advacnced_product_block"
      id="ProductSection"
      data-section-id="{{ section.id }}"
      data-section-type="product-template"
      data-image-zoom-type="{{ section.settings.product_image_zoom_type }}"
      data-show-extra-tab="{{ section.settings.show_extra_tab }}"
      data-extra-tab-content="{{ section.settings.extra_tab_content }}"
      data-enable-history-state="true"
    >
      {% case section.settings.add_to_cart_width %}
        {% when 'small' %}
          {%- assign btn_class = 'btn--wide' -%}
          {%- assign productform_class = 'product-form--wide' -%}
        {% when 'medium' %}
          {%- assign btn_class = 'btn--wide' -%}
          {%- assign productform_class = 'product-form--wide' -%}
        {% when 'large' %}
          {%- assign btn_class = 'btn--full' -%}
          {%- assign productform_class = 'product-form--full' -%}
      {% endcase %}

      {% if section.settings.add_to_cart_width != 'small' %}
        <style>
          @media screen and (min-width: 769px){
            .single-option-selector,
            .quantity-selector {
              {% if section.settings.add_to_cart_width == 'medium' %}
              min-width: 50%;
              {% else %}
              min-width: 100%;
              {% endif %}
            }
          }
        </style>
      {% endif %}

      <meta itemprop="url" content="{{ shop.url }}{{ product.url }}">
      <meta itemprop="image" content="{{ product.featured_image.src | img_url: 'grande' }}">

      <div class="product-single">
        <div class="medium--hide--title mobile_produ_info">
          <h2 class="mobile_heading advanced_product_title product-single__title" itemprop="name">
            {{ product.title }}
          </h2>
        </div>
        <div class="product-head">
          <div class="product-content-sec">
            <h1 class="small--hide advanced_product_title product-single__title" itemprop="name">
              {{ product.title }}
            </h1>
            <div itemprop="offers" itemscope itemtype="http://schema.org/Offer">
              {% assign variant = product.selected_or_first_available_variant %}
              <meta itemprop="priceCurrency" content="{{ cart.currency.iso_code }}">
              <link
                itemprop="availability"
                href="http://schema.org/{% if product.available %}InStock{% else %}OutOfStock{% endif %}"
              >

              <div
                class="small--hide price_part {% if product.available and product.variants.size > 1 %}mob-hidden{% endif %}"
                data-price="{{ variant.price | money_without_currency }}"
              >
                <span class="price_update">{{ variant.price | money }} </span>
                {% if variant.compare_at_price %}
                  <span id="ComparePrice" class="product-single__sale-price">
                    {{- variant.compare_at_price_max | money -}}
                  </span>
                {% endif %}
                {% if product.metafields.Stuhrling.MSRP != blank %}
                  <span class="product--value">MSRP: {{ product.metafields.Stuhrling.MSRP }}</span>
                {% endif %}

                {% if product.metafields.Stuhrling.MSRP_IN_EURO %}
                  <span class="product--value euro_msrp_metafield">
                    {{- product.metafields.Stuhrling.MSRP_IN_EURO -}}
                  </span>
                {% endif %}
                {% comment %}
                  {%if variant.compare_at_price > variant.price %}
                  <div class="grid-product__on-sale">
                    {% capture saved_amount %}{{ variant.compare_at_price | minus: variant.price | times: 100 | divided_by: variant.compare_at_price | round }}{% endcapture %}
                    {% capture saved_amounts %}{{ product.compare_at_price | minus: variant.price }}{% endcapture %}
                    <p>You save {{saved_amounts | money }} ({{ saved_amount }}% off)<span class="price_Sale_discount">Discounted price cannot be combined with other offers.</span></p>
                  </div>
                  {%endif%}
                {% endcomment %}
              </div>
              <div class="small--hide interest_message">4 interest-free payments of $98.75. Klarna. Learn more</div>
              <div class="small--hide product-single__prices{% if shop.taxes_included or shop.shipping_policy.body != blank %} product-single__prices--policy-enabled{% endif %}">
                <span id="PriceA11y" class="visually-hidden">{{ 'products.product.regular_price' | t }}</span>
                <span id="ProductPrice" class="product-single__price">{{ variant.price | money }}</span>
                {% include 'product-unit-price', variant: variant, available: true %}

                <!-- Placement v2 -->
                <klarna-placement
                  class="klarna_dynamic_msg"
                  data-key="credit-promotion-small"
                  data-locale="en-US"
                  data-purchase-amount=""
                ></klarna-placement>
                <!-- end Placement -->
              </div>

              <div class="small--hide star-review desktop_review star_rating">
                {% comment %}Start automatically added Judge.me widget{% endcomment %}
                {% include 'judgeme_widgets',
                  widget_type: 'judgeme_preview_badge',
                  concierge_install: true,
                  product: product
                %}
                {% comment %}End automatically added Judge.me widget{% endcomment %}
              </div>

              <!--
                <p class="affirm-as-low-as" data-page-type="product" data-amount="{{ current_variant.price | money }}"></p>
              -->
              <!--
                <a class="description_toggle_head {% if product.available and product.variants.size > 1 %}mob-hidden{% endif %}">View Product Features</a>
              -->
              <div
                class="product-des cription rte description_toggle {% if product.available and product.variants.size > 1 %}mob-hidden{% endif %}"
                itemprop="description"
                style="display:none;"
              >
                {{ product.description }}
              </div>
              {%- if shop.taxes_included or shop.shipping_policy.body != blank -%}
                <div class="hide product-single__policies{% if product.has_only_default_variant and section.settings.product_quantity_enable == false %} product__policies--no-dropdowns{% endif %} rte">
                  {%- if shop.taxes_included -%}
                    {{ 'products.general.include_taxes' | t }}
                  {%- endif -%}
                  {%- if shop.shipping_policy.body != blank -%}
                    {{ 'products.general.shipping_policy_html' | t: link: shop.shipping_policy.url }}
                  {%- endif -%}
                </div>
              {%- endif -%}

              {% form 'product', product, class: productform_class %}
                <select name="id" id="ProductSelect-{{ section.id }}" class="product-single__variants">
                  {% for variant in product.variants %}
                    {% if variant.available %}
                      <option
                        {% if variant == product.selected_or_first_available_variant %}
                          selected="selected"
                        {% endif %}
                        data-sku="{{ variant.sku }}"
                        value="{{ variant.id }}"
                      >
                        {{ variant.title }} - {{ variant.price | money_with_currency }}
                      </option>

                    {% else %}
                      <option disabled="disabled">{{ variant.title }} - {{ 'products.product.sold_out' | t }}</option>
                    {% endif %}
                  {% endfor %}
                </select>
                {% assign current_variant = product.selected_or_first_available_variant %}
                <div
                  class="small--hide price_part desk-hidden mobile_cart_price"
                  data-price="{{ variant.price | money_without_currency }}"
                >
                  <span class="price_update">{{ variant.price | money }} </span>
                  {% if variant.compare_at_price %}
                    <span id="ComparePrice" class="product-single__sale-price">
                      {{- variant.compare_at_price_max | money -}}
                    </span>
                  {% endif %}

                  {% if product.metafields.Stuhrling.MSRP != blank %}
                    <span class="product--value">MSRP: {{ product.metafields.Stuhrling.MSRP }}</span>
                  {% endif %}
                  {% if product.metafields.Stuhrling.MSRP_IN_EURO %}
                    <span class="product--value">{{ product.metafields.Stuhrling.MSRP_IN_EURO }}</span>
                  {% endif %}

                  {% if variant.compare_at_price > variant.price %}
                    <div class="grid-product__on-sale">
                      {% capture saved_amount %}{{ variant.compare_at_price | minus: variant.price | times: 100 | divided_by: variant.compare_at_price | round }}{% endcapture %}
                      {% capture saved_amounts %}{{ product.compare_at_price | minus: variant.price }}{% endcapture %}
                      <p>
                        You save {{ saved_amounts | money }} ({{ saved_amount }}% off)<span class="price_Sale_discount"
                          >Discounted price cannot be combined with other offers.</span
                        >
                      </p>
                    </div>
                  {% endif %}
                </div>
                <div class="{{current_variant.id}}"></div>
                {% if product.available and product.variants.size > 1 %}
                  <div class="small--hide button-center cart_price_btn_1 cart_fixed_btn">
                    <button
                      type="submit"
                      name="add"
                      id="AddToCart"
                      class="{% if current_variant.available and product.variants.size > 1 %}{% else %}sold_out_btn{% endif %} cart_fixed_add_to_cart btn {{ btn_class }}{% if section.settings.enable_payment_button %} btn--secondary{% endif %}"
                      data-price="{{ variant.price | money_without_currency |  plus: 0}}"
                    >
                      <span id="AddToCartText">
                        {%- if current_variant.available and product.variants.size > 1 -%}
                          {{- 'products.product.add_to_cart' | t -}}
                        {%- else -%}
                          {{- 'products.product.sold_out' | t -}}
                        {%- endif -%}
                      </span>
                      {{ variant.price | money }}
                    </button>
                  </div>
                {% endif %}

                <div class="mobile_bottom_content">
                  {% if product.available and product.variants.size > 1 %}
                    <div class="price_part desk-hidden" data-price="{{ variant.price | money_without_currency }}">
                      <div class="orginal_mobile_price">
                        <span class="price_update">{{ variant.price | money }} </span>
                        {% if variant.compare_at_price %}
                          |
                          <span id="ComparePrice" class="product-single__sale-price">
                            {{- variant.compare_at_price_max | money -}}
                          </span>
                        {% endif %}

                        <div class="msrp_mobile_price">
                          {% if product.metafields.Stuhrling.MSRP != blank %}
                            <span class="product--value">MSRP: {{ product.metafields.Stuhrling.MSRP }}</span>
                          {% endif %}
                          {% if product.metafields.Stuhrling.MSRP_IN_EURO %}
                            <span class="product--value euro_msrp_metafield">
                              {{- product.metafields.Stuhrling.MSRP_IN_EURO -}}
                            </span>
                          {% endif %}
                        </div>
                      </div>
                      {% if variant.compare_at_price > variant.price %}
                        <div class="grid-product__on-sale">
                          {% capture saved_amount %}{{ variant.compare_at_price | minus: variant.price | times: 100 | divided_by: variant.compare_at_price | round }}{% endcapture %}
                          {% capture saved_amounts %}{{ product.compare_at_price | minus: variant.price }}{% endcapture %}
                          <p>
                            You save {{ saved_amounts | money }} ({{ saved_amount }}% off)<span class="price_Sale_discount"
                              >Discounted price cannot be combined with other offers.</span
                            >
                          </p>
                        </div>
                      {% endif %}
                    </div>
                    <a class="small--hide description_toggle_head desk-hidden">View Product Features</a>
                    <div
                      class="product-description rte description_toggle desk-hidden"
                      itemprop="description"
                      style="display:none;"
                    >
                      {{ product.description }}
                    </div>
                  {% endif %}

                  <div class="mobile_info_outer">
                    <div class="mobile_info">
                      <div class="star-review mobile_review star_rating text-center">
                        {% comment %}Start automatically added Judge.me widget{% endcomment %}
                        {% include 'judgeme_widgets',
                          widget_type: 'judgeme_preview_badge',
                          concierge_install: true,
                          product: product
                        %}
                        {% comment %}End automatically added Judge.me widget{% endcomment %}
                      </div>

                      <div class="quant-buy-contain static_bar">
                        <div class="static_btn">
                          <button
                            data-ge-hide-countries="{% if product.tags contains 'Blocked_KW' %}KW US{% elsif product.tags contains 'Blocked_KW_US_AU_NZ' %}KW US AU NZ {% elsif product.tags contains 'Blocked_AU_NZ' %} AU NZ US{% elsif product.tags contains 'Blocked_ALL' %}ALL US{% else %}US{% endif %}"
                            type="submit"
                            name="add"
                            id="AddToCart"
                            class="btn {% if current_variant.available and product.variants.size > 1 %}{% else %}sold_out_btn{% endif %} {{ btn_class }}{% if section.settings.enable_payment_button %} btn--secondary{% endif %}"
                            data-price="{{ variant.price | money_without_currency |  plus: 0}}"
                          >
                            <span id="AddToCartText">
                              {% if current_variant.available and product.variants.size > 1 -%}
                                {{- 'products.product.add_to_cart' | t -}}
                              {%- else -%}
                                {{- 'products.product.sold_out' | t -}}
                              {%- endif -%}
                            </span>
                            {{ variant.price | money }}
                          </button>
                          <button
                            data-ge-show-countries="US"
                            type="submit"
                            name="add"
                            id="AddToCart"
                            class="btn {% if current_variant.available and product.variants.size > 1 %}{% else %}sold_out_btn{% endif %} {{ btn_class }}{% if section.settings.enable_payment_button %} btn--secondary{% endif %}"
                            data-price="{{ variant.price | money_without_currency |  plus: 0}}"
                          >
                            <span id="AddToCartText">
                              {% if current_variant.available and product.variants.size > 1 -%}
                                {{- 'products.product.add_to_cart' | t -}}
                              {%- else -%}
                                {{- 'products.product.sold_out' | t -}}
                              {%- endif -%}
                            </span>
                            {{ variant.price | money }}
                          </button>
                          <button
                            disabled
                            data-ge-show-countries="{% if product.tags contains 'Blocked_KW' %}KW{% elsif product.tags contains 'Blocked_KW_US_AU_NZ' %}KW US AU NZ {% elsif product.tags contains 'Blocked_AU_NZ' %} AU NZ  {% elsif product.tags contains 'Blocked_ALL' %}ALL{% endif %}"
                            type="submit"
                            name="add"
                            id="AddToCart"
                            class="btn {% if current_variant.available and product.variants.size > 1 %}{% else %}sold_out_btn{% endif %} {{ btn_class }}{% if section.settings.enable_payment_button %} btn--secondary{% endif %}"
                            data-price="{{ variant.price | money_without_currency |  plus: 0}}"
                          >
                            <span id="AddToCartText">
                              {% if current_variant.available and product.variants.size > 1 -%}
                                {{- 'products.product.add_to_cart' | t -}}
                              {%- else -%}
                                {{- 'products.product.sold_out' | t -}}
                              {%- endif -%}
                            </span>
                            {{ variant.price | money }}
                          </button>
                          <div
                            class="error_message"
                            style="color:red; font-weight:600;"
                            data-ge-show-countries="{% if product.tags contains 'Blocked_KW' %}KW{% elsif product.tags contains 'Blocked_KW_US_AU_NZ' %}KW US AU NZ {% elsif product.tags contains 'Blocked_AU_NZ' %} AU NZ {% elsif product.tags contains 'Blocked_ALL' %}ALL{% endif %}"
                          >
                            <i class="fa fa-times-circle"></i> We are sorry, but you are unable to purchase this product
                            in the selected country due to restrictions.
                          </div>
                        </div>
                        <div class="button-center cart_price_btn fixed_btn ">
                          <button
                            data-ge-hide-countries="{% if product.tags contains 'Blocked_KW' %}KW US{% elsif product.tags contains 'Blocked_KW_US_AU_NZ' %}KW US AU NZ {% elsif product.tags contains 'Blocked_AU_NZ' %} AU NZ US{% elsif product.tags contains 'Blocked_ALL' %}ALL US{% else %}US{% endif %}"
                            type="submit"
                            name="add"
                            id="AddToCart"
                            class="btn {% if current_variant.available and product.variants.size > 1 %}{% else %}sold_out_btn{% endif %} {{ btn_class }}{% if section.settings.enable_payment_button %} btn--secondary{% endif %}"
                            data-price="{{ variant.price | money_without_currency |  plus: 0}}"
                            style="display:none;"
                          >
                            <span id="AddToCartText">
                              {% if current_variant.available and product.variants.size > 1 -%}
                                {{- 'products.product.add_to_cart' | t -}}
                              {%- else -%}
                                {{- 'products.product.sold_out' | t -}}
                              {%- endif -%}
                            </span>
                            {{ variant.price | money }}
                          </button>
                          <button
                            data-ge-show-countries="US"
                            type="submit"
                            name="add"
                            id="AddToCart"
                            class="btn {% if current_variant.available and product.variants.size > 1 %}{% else %}sold_out_btn{% endif %} {{ btn_class }}{% if section.settings.enable_payment_button %} btn--secondary{% endif %}"
                            data-price="{{ variant.price | money_without_currency |  plus: 0}}"
                          >
                            <span id="AddToCartText">
                              {% if current_variant.available and product.variants.size > 1 -%}
                                {{- 'products.product.add_to_cart' | t -}}
                              {%- else -%}
                                {{- 'products.product.sold_out' | t -}}
                              {%- endif -%}
                            </span>
                            {{ variant.price | money }}
                          </button>
                          <button
                            disabled
                            data-ge-show-countries="{% if product.tags contains 'Blocked_KW' %}KW{% elsif product.tags contains 'Blocked_KW_US_AU_NZ' %}KW US AU NZ {% elsif product.tags contains 'Blocked_AU_NZ' %} AU NZ  {% elsif product.tags contains 'Blocked_ALL' %}ALL{% endif %}"
                            type="submit"
                            name="add"
                            id="AddToCart"
                            class="btn {% if current_variant.available and product.variants.size > 1 %}{% else %}sold_out_btn{% endif %} {{ btn_class }}{% if section.settings.enable_payment_button %} btn--secondary{% endif %}"
                            data-price="{{ variant.price | money_without_currency |  plus: 0}}"
                            style="display:none;"
                          >
                            <span id="AddToCartText">
                              {% if current_variant.available and product.variants.size > 1 -%}
                                {{- 'products.product.add_to_cart' | t -}}
                              {%- else -%}
                                {{- 'products.product.sold_out' | t -}}
                              {%- endif -%}
                            </span>
                            {{ variant.price | money }}
                          </button>
                          <div
                            class="error_message"
                            style="color:red; font-weight:600;"
                            data-ge-show-countries="{% if product.tags contains 'Blocked_KW' %}KW{% elsif product.tags contains 'Blocked_KW_US_AU_NZ' %}KW US AU NZ {% elsif product.tags contains 'Blocked_AU_NZ' %} AU NZ {% elsif product.tags contains 'Blocked_ALL' %}ALL{% endif %}"
                            style="display:none;"
                          >
                            <i class="fa fa-times-circle"></i> We are sorry, but you are unable to purchase this product
                            in the selected country due to restrictions.
                          </div>
                        </div>
                      </div>

                      <div
                        id="splitit-container"
                        data-splitit="true"
                        data-splitit-amount="{{ current_variant.price | divided_by: 100.0 | json }}"
                        data-splitit-num-installments="5"
                        data-splitit-type="product-description"
                        class="interest_message splitt_message"
                      ></div>
                    </div>
                    {% render 'swatch' with 'Color' as swatch %}
                  </div>
                </div>
                <div class="shipping-Strap">
                  {% if product.tags contains 'Strap' %}
                    {% include 'strap_text' %}
                  {% endif %}
                </div>

                {% if section.settings.enable_payment_button %}
                  {{ form | payment_button }}
                {% endif %}
              {% endform %}

              {% unless section.settings.show_extra_tab == false
                or pages[section.settings.extra_tab_content] == empty
              %}
                <div class="tabs">
                  <ul class="tab-switch__nav">
                    <li>
                      <a href="#description" data-link="description" class="tab-switch__trigger h3">
                        {{- 'products.product.description' | t -}}
                      </a>
                    </li>
                    <li class="extra_tab">
                      <a href="#extra" data-link="extra" class="tab-switch__trigger h3">
