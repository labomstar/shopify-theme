<style>
  .btn.banner-btn {
    line-height: normal;
    min-height: unset;
    margin: 0 auto;
    display: block;
    position: absolute;
    bottom: 11%;
    left: 50%;
    transform: translate(-50%, 0px);
}
  .banner_content_block {
    position: absolute;
/*     top: 5%; */
    left: 50%;
    transform: translate(-50%, 0%);
/*     font-weight: bold; */
    width:100%;
    bottom: 105px;
}
  .custom_image_block {
    position: relative;
}
  .image_sub_title_head, .image_title_head
  {
        text-transform: uppercase;
        font-weight: 300;
    font-size: 25px;
    line-height: 35px;
  }
