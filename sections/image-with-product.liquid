<div class="block banner_block custom_block"> 
<div class="banner-section-outer custom_outer_section grid">   
{% for block in section.blocks %}
        {% case block.settings.width %}
          {% when '25%' %}
            {%- assign block_width = 'col-xs-12 col-md-3' -%}
          {% when '33%' %}
            {%- assign block_width = 'col-xs-12 col-md-4' -%}
      	  {% when '41%' %}
            {%- assign block_width = 'col-xs-12 col-md-5' -%}
          {% when '50%' %}
            {%- assign block_width = 'col-xs-12 col-md-6' -%}
      	  {% when '58%' %}
            {%- assign block_width = 'col-xs-12 col-md-7' -%}
          {% when '66%' %}
            {%- assign block_width = 'col-xs-12 col-md-8' -%}
          {% when '75%' %}
            {%- assign block_width = 'col-xs-12 col-md-9' -%}
          {% when '100%' %}
            {%- assign block_width = 'col-xs-12 col-md-12' -%}
        {% endcase %}
<div class="banner-section grid__item product_img_item custom_item custom__item--{{ block.id }} small--one-whole {{ block_width }} {% if block.settings.alignment %}align--{{ block.settings.alignment }}{% endif %}" {{ block.shopify_attributes }}>
  <div class="custom__item-inner custom__item-inner--{{ block.type }}">
      {% case block.type %}
              {% when 'image' %}
    {% if block.settings.image != blank %}
    <div class="custom_image_block">
      {% if block.settings.image_url != blank %}  <a href="{{ block.settings.image_url }}">{% endif %}
      <div class="custom__block-image-container image-{{ forloop.index }}">
        {% assign img_url = block.settings.image | img_url: 'master' | replace: '_original.', '_{width}x.' %}

        <img class="custom__block-image lazyload"
             data-src="{{ img_url }}"
             data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]"
             data-aspectratio="{{ block.settings.image.aspect_ratio }}"
             data-sizes="auto"
             alt="{{ block.settings.image.alt | escape }}" sizes="auto">                    
        <noscript>
          <img src="{{ block.settings.image | img_url: 'master' }}" alt="{{ block.settings.image.alt | escape }}" class="custom__block-image">
        </noscript>

      </div>
      {% if block.settings.image_title != blank or block.settings.image_sub_title != blank  or block.settings.image_short_desc != blank or block.settings.button_title != blank  %}
      <div class="banner_content_block img_cont">
        {% if block.settings.image_title != blank %} <h2 style="color:{{ block.settings.text_color}};" class="image_title_head {% if block.settings.alignment %}align--{{ block.settings.alignment }}{% endif %}">{{  block.settings.image_title }}</h2>{% endif %}
        {% if block.settings.image_sub_title != blank %} <h4 style="color:{{ block.settings.text_color}};" class="image_sub_title_head {% if block.settings.alignment %}align--{{ block.settings.alignment }}{% endif %}">{{  block.settings.image_sub_title }}</h4>{% endif %}
        {% if block.settings.image_short_desc != blank %} <p style="color:{{ block.settings.text_color}};" class="image_text {% if block.settings.alignment %}align--{{ block.settings.alignment }}{% endif %}">{{  block.settings.image_short_desc }}</p>{% endif %}       
      </div>
       {% if block.settings.button_title != blank %} <a style="border:1px solid {{ block.settings.button_bg }}; background-color:{{ block.settings.button_bg }}; color:{{ block.settings.button_color}};" class="{% if block.settings.alignment %}align--{{ block.settings.alignment }}{% endif %} btn banner-btn banner-button" href="{{ block.settings.button_link}}">{{  block.settings.button_title }}</a>{% endif %}
      {% endif %}
        {% if block.settings.image_url != blank %}  </a>{% endif %}     
      </div>

    {% else %}
    {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
    {% endif %}
        {% when 'Collection' %} 
       <div id="tab{{ forloop.index }}" class="tab-content-fac">
          {%- assign collection = collections[block.settings.collection] -%}
          <div class="sec_fect">
          <div class="grid--fac">
          {% for product in collection.products limit: 2 %} 
           <div class="grid-product-pro">
              <a class="product_img_link" href="{{ product.url }}">
                <div class="reveals">
                  {% if product.compare_at_price > product.price and settings.promotion_tag %}
                  <div class="product_discount" aria-hidden="true" accesskey>
                    {% if settings.promo_text_or_pourc == '1' %}
                      {{settings.promo_text}}
                    {% else %}
                      {% if product.compare_at_price_max != product.compare_at_price_min %}
                        {% assign compare_old_count = 0 %}
                        {% assign compare_new_count = 0 %}
                        {% for variant in product.variants %}
                          {% if variant.compare_at_price > variant.price %}
                              {% capture compare_new_count %}{{ variant.compare_at_price | minus: variant.price | times: 100.0 | divided_by: variant.compare_at_price| ceil }}{% endcapture %}
                            <script>if({{compare_new_count}} > {{compare_old_count}}){ {% assign compare_old_count = compare_new_count %} }</script>
                          {% endif %}
                        {% endfor %}
                        <span>{{ 'products.product.upto' | t }} -{{ compare_old_count | append: "%"}}</span>
                      {% else %}
                        <span>-{{ product.compare_at_price | minus: product.price | times: 100.0 | divided_by: product.compare_at_price | ceil }}%</span>
                      {% endif %}
                    {% endif %}
                  </div>
                {% endif %}
                  {% if product.images.size > 1 %}
                  <img class="product_img_new" src="{{ product.featured_image.src | img_url: 'original' }}">
                  {% else %}
                   <img src="{{ product.featured_image.src | img_url: 'original' }}" alt="{{ product.featured_image.alt | escape }}">
            {% endif %}
                </div>
              </a>
              <a class="product_title" href="{{ product.url }}">{{ product.title }}</a>
              {% if product.available %}
              <div class="product-card__price">
                {% if product.compare_at_price > product.price %}
                  {% if product.price_varies %}
                    {% assign sale_price = product.price | money %}
                    <span class="product-card__promo-price">{{ 'products.product.on_sale_from_html' | t: price: sale_price }}</span>
                  {% else %}
                    <span class="product-card__promo-price">{{ product.price | money }}</span>
                    <s class="product-card__regular-price">{{ product.compare_at_price | money }}</s>
                  {% endif %}
                {% else %}
                  {% if product.price_varies %}
                    {% assign price = product.price | money %}
                    {{ 'products.product.from_text_html' | t: price: price }}
                  {% else %}
                    {{ product.price | money }}
                  {% endif %}
                {% endif %}
                {%- unless product.price_varies -%}
                  {%- if current_variant.unit_price_measurement -%}
                    {% include 'product-unit-price', product_variant: current_variant, wrapper_class: "product-card__unit-price" %}
                  {%- endif -%}
                {%- endunless -%}
                <form class="productform_class">
            <input type="hidden" name="id" value="{{ product.variants.first.id }}" class="product-single__variants"/>
           <select name="id" id="ProductSelect-{{ section.id }}" class="product-single__variants">
              {% for variant in product.variants %}
                {% if variant.available %}

                  <option {% if variant == product.selected_or_first_available_variant %} selected="selected" {% endif %} data-sku="{{ variant.sku }}" value="{{ variant.id }}">{{ variant.title }} - {{ variant.price | money_with_currency }}</option>

                {% else %}
                  <option disabled="disabled">
                    {{ variant.title }} - {{ 'products.product.sold_out' | t }}
                  </option>
                {% endif %}
              {% endfor %}
            </select>
            
            <div class="product-single__quantity{% unless section.settings.product_quantity_enable %} is-hidden{% endunless %}">
              <label for="Quantity">{{ 'products.product.quantity' | t }}</label>
              <input type="number" id="Quantity" name="quantity" value="1" min="1" class="quantity-selector">
            </div>
         	
            
       
         <div class="button-center  AddToCartButton">	 
             <button   data-ge-show-countries="US"  type="submit" name="add" class="AddToCart" id="AddToCart" class="us-selected-country-us btn {% if current_variant.available and product.variants.size > 1 %}{% else %}sold_out_btn{% endif %} {{ btn_class }}{% if section.settings.enable_payment_button %} btn--secondary{% endif %}" data-price="{{ variant.price | money_without_currency |  plus: 0}}" style="display: block;">	
                <span id="AddToCartText">    {% if product.variants.first.id.size > 1 %}{{ 'products.product.add_to_cart' | t }}{% else %}{{ 'products.product.sold_out' | t }}{% endif %}</span>	
                {{ variant.price | money }}	
              </button> 
           
         
              
            </div>
    	  

          {% if section.settings.enable_payment_button %}
            {{ form | payment_button }}
          {% endif %}
</form>
              </div>
            {% else %}
              <div class="product-card__availability">
                {{ 'products.product.sold_out' | t }}
              </div>
            {% endif %}

            </div>
          {%endfor%}
          </div>
          </div>
        </div>
     {% endcase %}
  </div>
</div>
            
  {% endfor %}
</div>
</div>
  <style>
    .grid--fac {
    display: flex;
}
    .grid-product-pro {
    width: 50%;
    padding: 0 60px;
}
    img.custom__block-image {
    background-image: linear-gradient(to bottom right, #fff , #000 50%);
}
    .grid-product-pro {
    display: flex;
    flex-direction: column;
          text-align: center;
}
    .product-card__price {
    margin-top: auto;
}
  .reveals {
    width: 100%;
    padding: 30px;
}
    
  </style>
