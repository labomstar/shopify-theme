    .banner-section:first-child {
      margin-bottom: 20px;
    }
    .block.banner_block
    {
          padding-right: 0px;
    }
    div.cart_price_btn {
    position: unset !important;
    }
    .img_cont h2 {
    color: #000 !important;
}
    .img_cont p {
    color: #000 !important;
}
    .btn.banner-btn.banner-button {
    color: #f60 !important;
}
    .grid-product-pro {
    padding: unset !important;
}
      .block.banner_block.custom_block {
    overflow: hidden;

}
    .product_title, .product_money {
    font-size: 13px;
    padding: 0 10px !important;
    text-align: center !important;
}
  }
  @media (max-width:480px)
    {
      .banner_content_block
      {
        top:0;
      }
      .image_sub_title_head 
      {
        font-size: 14px;
      }
      button#AddToCart {
    font-size: 15px !important;
}
      .product_img_item {
    padding: 0 !important;
}
    }
  @media (max-width:414px)
  {
    .image_title_head {
      font-size: 25px;
    }
    
  }
  @media(max-width:375px)
