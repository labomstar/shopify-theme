    background-color: #000 !important;
    border-color: #000 !important;
} */
  .block.banner_block
  {
    padding-right:20px;
  }
  .custom_block {
    padding: 40px 0 !important;
}
  .custom_item {
    padding: 0;
}

.custom__block-image-container {
    overflow: hidden;
}

img.custom__block-image {
    transform: unset;
    object-fit: cover;
    display: block;
}
  .banner-button {
    border: 2px solid #ff6b00 !important;
}

  @media(max-width:768px)
