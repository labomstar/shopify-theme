        <div class="accordion_body menu-footer-shop-container" style="display: none;">
          <ul id="menu-footer-shop" class="menu">
            {% for link in linklists[section.settings.menu_1].links %}
            <li id="menu-item-31507" class="menu-item">
                <a href="{{ link.url }}">
                  {% assign lowerCaseTitle = link.title | downcase %}
                  {% if lowerCaseTitle == 'mens' %}
                    MEN'S
                  {% else %}
                    {{ link.title }}
                  {% endif %}
                </a>
            </li>
             {%endfor%}
          </ul>
        </div>
      </div>
      <div id="nav_menu-7" class="col pb-0 widget widget_nav_menu">
           <div class="accordion_head widget-title">Services<span class="plusminus"><i class="fa fa-angle-down"></i></span></div>
          <div class="accordion_body menu-footer-services-container" style="display: none;">
         <ul id="menu-footer-shop" class="menu">
        {% for link in linklists[section.settings.menu_2].links %}
          <li id="menu-item-31507" class="menu-item">
            <a href="{{ link.url }}">{{ link.title }}</a>
          </li>
        {%endfor%}
