<footer id="footer" class="footer-wrapper">
  <div class="wrapper-mini">
  <div class="footer-widgets footer footer-2 dark">
    <div class="row dark large-columns-6 mb-0">
      
       <div id="block_widget-7" class="col pb-0 widget block_widget desktop"> 
        <img class="lazyload" src="data:image/gif;base64,R0lGODlhAQABAIAAAP///wAAACH5BAEAAAAALAAAAAABAAEAAAICRAEAOw==" data-src="{%if section.settings.image != blank %}{{section.settings.image | img_url:'master'}}{%endif%}">
        <div class="whatsapp_number">
          </span> 
          <span>
          </span>
        </div>
        <div>
          <span class="footer-phone-icon" style="float: left; margin-right:18px; margin-top:8px;"><i class="fa fa-phone" style="font-size: 30px"></i></span> 
          <span>
            <div style="font-size:14px; font-weight:600; letter-spacing: .05em; color: #6a6a6a;"><a href="tel:{{section.settings.phone-number}}">{{section.settings.phone-number}}</a> / <a href="tel:{{section.settings.phone-number-2}}">{{section.settings.phone-number-2}}</a></div>
            <div style="font-size: .8em; margin-top:-3px; color:#000;">.</div> 
          </span>
        </div>
        <div class="email_block">
          <p style="font-size:14px; font-weight:600; letter-spacing: .05em;margin-top:10px;">
            <i class="fa fa-envelope-o footer-email-icon" style="font-size:25px; margin-right: 14px;"></i><a href="mailto:{{section.settings.email}}" class="emails">EMAIL US</a>
          </p>
        </div>
        
      </div>
      <div id="block_widget-5" class="col pb-0 widget block_widget desktop">
        <span class="widget-title">Signup for <span class="sign-up-newsletter-title-our">Our </span>Newsletter</span>
        <div class="is-divider small"></div>
        <p style="">Get the latest news, invites and exclusive offers straight to your inbox.</p>
           <div class="newsletter-content">
        {% form 'customer' %}
          {{ form.errors | default_errors }}
          {% if form.posted_successfully? %}
            <p class="note form-success">{{ 'general.newsletter_form.confirmation' | t }}</p>
          {% else %}
            <input type="hidden" name="contact[tags]" value="newsletter">
            <div class="input-group">
              <label for="Email" class="visually-hidden">{{ 'contact.form.email' | t }}</label>
              <input type="email" value="{% if customer %}{{ customer.email }}{% endif %}" placeholder="{{ 'general.newsletter_form.newsletter_email' | t }}" name="contact[email]" id="Email" class="input-group-field" aria-label="{{ 'general.newsletter_form.newsletter_email' | t }}" autocorrect="off" autocapitalize="off">
              <span class="input-group-btn">
                <button type="submit" class="btn" name="commit" id="subscribe">{{ 'general.newsletter_form.submit' | t }}</button>
              </span>
            </div>
          {% endif %}
        {% endform %}
      </div>
        <br>
     <div class="social-icons follow-icons full-width text-left">
       <ul class="inline-list social-icons">
         <li>
           <a class="icon-fallback-text" target="_blank" href="{{ settings.social_facebook_link | escape }}">
           <i class="fa fa-facebook-f"></i>
           </a>
         </li>
         <li>
           <a class="icon-fallback-text" target="_blank" href="{{ settings.social_instagram_link | escape }}">
           <i class="fa fa-instagram" aria-hidden="true"></i>
           </a>
         </li>
         <li>
            <a class="icon-fallback-text" target="_blank" href="{{ settings.social_twitter_link | escape }}">
           <i class="fa fa-twitter" aria-hidden="true"></i>
           </a>
         </li>
         <li>
             <a class="icon-fallback-text" target="_blank" href="{{ settings.social_pinterest_link | escape }}">
           <i class="fa fa-pinterest-p"></i>
           </a>
         </li>
         <li>    <a class="icon-fallback-text" target="_blank" href="{{ settings.social_rss_link | escape }}">
           <i class="fa fa-linkedin-square"></i>
           </a>
         </li>
         <li>
            <a class="icon-fallback-text" target="_blank" href="{{ settings.social_youtube_link | escape }}">
           <i class="fa fa-youtube-play"></i>
           </a>
         </li>
       </ul>

      </div>
      </div>
