
<div class="block css-review">
  <div class="wrapper-mini">
    
  <div class="review__header">
    {%if section.settings.Customer != blank %} <div class="customer_review_title"><span class="csutomer">{{section.settings.Customer }}</span></div>{%endif%}
      <div class="review_total_count"> 
                <p class="judgeme_total_reviews">{%if section.settings.Average != blank %}{{section.settings.Average }}{%endif%}</p>
        <p class="starts"><span>REVIEWS</span><i class="fa fa-star" aria-hidden="true"></i><i class="fa fa-star" aria-hidden="true"></i><i class="fa fa-star" aria-hidden="true"></i><i class="fa fa-star" aria-hidden="true"></i><i class="fa fa-star-half-o" aria-hidden="true"></i></p>
<!--         <p class="judgeme_total_reviews">{{ shop.metafields.judgeme.shop_reviews_count }}+ REVIEWS</p> -->
      </div>
     {%if section.settings.Read-url != blank %}
       <div class="read__btn"><a class="read_them" href="{{section.settings.Read-url }}">{%if section.settings.Read != blank %}{{section.settings.Read }}{%else%}Read Them{%endif%}</a></div>
  {% endif %}
       </div> 
    
  <div class="review-center desktop desktop_reviews_block">
