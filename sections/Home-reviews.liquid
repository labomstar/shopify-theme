<style>
  .review__header {
    justify-content: center;
}
  .review_total_count .judgeme_total_reviews {
    font-size: 30px;
    line-height: 10px;
    color: #f37c2a;
        letter-spacing: 6px;

}
  .review-center .slick-dots{
    display: none !important;
}
  @media (max-width:480px)
    {
  .review_total_count {
    width: 100%;
}
      .css-review {
    padding-bottom: 0px;
}

      .review_total_count .judgeme_total_reviews{
line-height: normal;
}
.starts{
margin: 0 !important;
}

.slick-dotted.slick-slider {
margin-bottom: 0 !important;
}

.mobslider-collection .slick-list {
margin-bottom: 0;
}

.text-consider {
color: #000;
font-weight: 400;
font-size: 16px;
}

.img_cont h2{
font-weight: 500;
}

.block {
padding: 40px 25px 20px 30px;
}
      
    }
</style>

