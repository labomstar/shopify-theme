<script>
  $(document).on('click', '.title_toggle', function(){
    if ($('.discount_block').is(':visible')) {
      $(".discount_block").slideUp(300);
      $(".plus_minus_discount").text('+');
    }
    if ($(this).next(".discount_block").is(':visible')) {
      $(this).next(".discount_block").slideUp(300);
      $(this).children(".plus_minus_discount").text('+');
    } else {
      $(this).next(".discount_block").slideDown(300);
      $(this).children(".plus_minus_discount").text('-');
    }

  });
  
//   Change Desktop quantity on the basis of Mobile
  $('.mobile_qty_block input').change(function() {
    var qty_id = $(this).attr('id');
    var qty_val = $(this).val();
    $('.cart__row').each(function(){
      var desk_qty_id = $(this).find('.desktop-qty-block.qty-area input').attr('id');
      if(desk_qty_id == qty_id )
      {
        $(this).find('.desktop-qty-block.qty-area input').val(qty_val);
      }
    })

  });
</script>