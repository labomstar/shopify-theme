{% comment %}

  This is your /cart template.

{% endcomment %}
<div id="CartSection" data-section-id="{{ section.id }}" data-section-type="cart-template" data-notes-enable="{{ section.settings.cart_notes_enable }}">
 <div id="progress-bar-widget" class="progress-bar-widget
                                          {% if cart.item_count > 0 %} display-block {% endif %}" data-threshold="35000">
       {% assign temp = cart.total_price | times: 100 %}
       {% assign progress_percent = temp | divided_by: 35000 %}
       {% if progress_percent > 100 %}
         {% assign progress_percent = 100 %}
       {% endif %}
       <div id="progress-bar" class="cart-drawer__progress-bar" >
         <div class="progress" style="width: {{- progress_percent -}}%"> </div>
       </div>
       <p class="progress_info">
          {% if cart.total_price >= 35000 %} 
          You've earned a <b>3 watch travel case</b> for free, use Code: <b>FREETRAVEL</b>
          {% else %}
          You are <b class="progress_price">{{ 35000 | minus: cart.total_price | money }}</b> away from getting the 3 watch travel case for free!
          {% endif %}
       </p>
     </div>  



{% if cart.item_count > 0 %}
<div class="cart_shipping_bar">
		    <i class="fa fa-check" aria-hidden="true"></i> 
        <span>You've earned free shipping!</span>
      </div>
    <form action="{{ routes.cart_url }}" method="post" novalidate class="cart">

    
      {% comment %}
        Loop through products in the cart
      {% endcomment %}
      <div class="max-wrapper cart_outer">
        <div class="container custom_cart_container">
          <div class="row align-rows">
            <div class="col-md-7 col-sm-12 col-xs-12 cart__header">
              <div class="cart__row medium-down--hide cart__header-labels">
        <div class="row-set">
          
            <div class="row">
              <div class="col-md-6 col-sm-6">
              <div class="col-md-6 col-sm-6 label-PRODUCT">
                <span class="h4">PRODUCT</span>
              </div>
              <div class="col-md-6 col-sm-6">
                <span class="h4">&nbsp;</span>
              </div>
              </div>
              <div class="col-md-6 col-sm-6 label-section">
              <div class="col-md-4 col-sm-4">
                <span class="h4">{{ 'cart.label.price' | t }}</span>
              </div>
              <div class="col-md-4 col-sm-4">
                <span class="h4">{{ 'cart.label.quantity' | t }}</span>
              </div>
              <div class="col-md-4 col-sm-4">
                <span class="h4">{{ 'cart.label.total' | t }}</span>
              </div>
              </div>
            </div>
        </div>
      </div>
               <button type="submit" name="update" class="cart_btn btn btn--secondary update-cart">{{ 'cart.general.update' | t }}</button>              
              {% for item in cart.items %}
              <div class="cart__row {% if forloop.index %}first_cart_row{% endif %}">
<!--                Add   {% if item.variant.metafields.Stuhrling.upsell_product_1  != blank %}cart_upsells_space{% endif %} to row div if variant upsells will be added -->
                <div class="row ">

                  <div class="col-sm-6 col-md-6 col-xs-12 cart_row">
                    <div class="grid left-margin">

                      <div class="grid__item one-third left-padding">
                      
                        <a href="{{ item.url | within: collections.all }}">
                          {% unless item.image == blank %}
                          {% capture img_id %}ProductImage-{{ item.id }}{% endcapture %}
                          {% capture wrapper_id %}ProductImageWrapper-{{ item.id }}{% endcapture %}
                          {%- assign img_url = item.image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
                          {% include 'image-style' with image: item.image, width: 210, height: 245, wrapper_id: wrapper_id, img_id: img_id %}
                          <!-- Extend -- Disable link to warranty page -->	
                          <div 	
                               id="{{ wrapper_id }}" 	
                               class="cart__image-wrapper supports-js"	
                               {% if item.vendor == 'Extend' %}style="pointer-events:none;"{% endif %}>	
                            <!-- Extend -- End Extend code -->
                            
                              <img id="{{ img_id }}"
                                   class="cart__image1 lazyload"
                                   data-src="{{ img_url }}"
                                   data-widths="[40, 65, 90, 120, 150, 180, 360, 480, 600]"
                                   data-aspectratio="{{ item.image.aspect_ratio }}"
                                   data-sizes="auto"
                                   alt="{{ item.title | escape }}">
                          </div>
                          {% else %}
                          <p><img src="{{ item | img_url: 'medium' }}" alt="{{ item.title | escape }}"></p>
                          {% endunless %}

                          <noscript>
                            <p><img src="{{ item | img_url: 'medium' }}" alt="{{ item.title | escape }}"></p>
                          </noscript>
                        </a>
                      </div>

                      <div class="grid__item two-thirds ">
                         <!-- Extend -- Disable link to warranty page -->	
                        <a 	
                           href="{{ item.url }}" 	
                           class="h5 cart__product-title"	
                           {% if item.vendor == 'Extend' %}style="pointer-events:none;"{% endif %}>
                          {{ item.product.title }}
                        </a>
                        <!-- Extend -- End Extend code -->	
                        <!-- Extend -- Hide warranty metadata -->	
                        {% unless item.variant.title contains 'Default' or item.vendor == 'Extend' %}	
                        <!-- Extend -- End Extend code -->
                        <br>
                       {% comment %} <small>{{ item.variant.title }}</small>{% endcomment %}
                        {% endunless %}
                        {% comment %}
                        Optional, loop through custom product line items if available

                        For more info on line item properties, visit:
                        - http://docs.shopify.com/support/your-store/products/how-do-I-collect-additional-information-on-the-product-page-Like-for-a-monogram-engraving-or-customization
                        {% endcomment %}
                        {% assign propertySize = item.properties | size %}
                        {% if propertySize > 0 %}
                        {% for p in item.properties %}
                        {% assign first_character_in_key = p.first | truncate: 1, '' %}
                       <!-- Extend -- Hide warranty metadata -->	
                        {% unless p.last == blank or first_character_in_key == '_' or p.first == 'Ref' or p.first contains 'Extend.' %}	
                        <!-- Extend -- End Extend code -->
                        <br>
                         <!-- Extend -- Style product and term details with a 'small' element -->	
                        <small>
                        {{ p.first }}:

                        {% comment %}
                        Check if there was an uploaded file associated
                        {% endcomment %}
                        {% if p.last contains '/uploads/' %}
                        <a href="{{ p.last }}">{{ p.last | split: '/' | last }}</a>
                        {% else %}
                        {{ p.last }}
                        {% endif %}

                        {% endunless %}
                            </small>	
                        <!-- Extend -- End Extend code -->
                        {% endfor %}
                        {% endif %}
                        <div class="col-sm-4 col-md-4 col-xs-12 price-area mobile_price_block">	
<!--                           <span class="cart__mini-labels">{{ 'cart.label.price' | t }}</span> -->	
                          {%- if item.original_price != item.final_price -%}	
                          	
                          <del class="h5">{{ item.original_price | money }}</del>	
                          <span class="visually-hidden">{{ 'products.product.sale_price' | t }}</span>	
                          <span class="order-discount order-discount--price h5">{{ item.final_price | money }}</span>	
                          {%- else -%}	
                          <span class="h5">	
                            {%if item.variant.compare_at_price > item.original_price %}	
                            <div class="pricing_with_sale">	
                              <span class="sale_pricing">	
                                {{ item.original_price | money }}	
                                <span class="compare_price">{{ item.variant.compare_at_price  | money }}</span> 
                              </span>	
                              <div class="col-sm-3 col-md-3 col-xs-4 qty-area mobile_qty_block">	
                                
<!--                                                           <label for="updates_{{ item.key }}" class="cart__mini-labels" style="display:none;">{{ 'cart.label.quantity' | t }}</label>	
                                <input type="number" class="cart__quantity-selector" name="updates[]" id="updates_{{ item.key }}" value="{{ item.quantity }}" min="0" aria-label="{{ 'cart.label.quantity' | t }}">	 -->
                              </div>
                              {% capture saved_amount %}{{ item.variant.compare_at_price | minus: item.original_price | times: 100 | divided_by: item.variant.compare_at_price | round }}{% endcapture %}	
                              {% capture saved_amounts %}{{ item.variant.compare_at_price | minus: item.original_price }}{% endcapture %}	
                              <div class="save_price">You save &nbsp; {{saved_amounts | money }} 	
                                <span class="save_percentage">({{ saved_amount }}% off)</span>	
                              </div>	
                            </div>	
                            {% else %}	
                            {{ item.original_price | money }}	
                            
                            {% endif %}	
                          </span>	
                          {%- endif -%}	
                          {%if item.variant.compare_at_price > item.original_price %}	
                          {% else %}
                          <div class="col-sm-3 col-md-3 col-xs-4 qty-area mobile_qty_block">	
                                <label for="updates_{{ item.key }}" class="cart__mini-labels" style="display:none;">{{ 'cart.label.quantity' | t }}</label>	
                                <input type="number" class="cart__quantity-selector" name="mobile_update[]" id="updates_{{ item.key }}" value="{{ item.quantity }}" min="0" aria-label="{{ 'cart.label.quantity' | t }}">	
                              </div>
                          {% endif %}
                          {%- if item.variant.available and item.variant.unit_price_measurement -%}	
                          {% include 'product-unit-price', variant: item, available: item.variant.available %}	
                          {%- endif -%}	
                          {%- if item.line_level_discount_allocations != blank -%}	
                          <ul class="order-discount order-discount--list order-discount--title order-discount--cart medium-down--hide" aria-label="{{ 'customer.order.discount' | t }}">	
                            {%- for discount_allocation in item.line_level_discount_allocations -%}	
                            <li class="order-discount__item">	
                              <span class="icon icon-saletag" aria-hidden="true"></span>{{ discount_allocation.discount_application.title }} (-{{ discount_allocation.amount | money }})	
                            </li>	
                            {%- endfor -%}	
                          </ul>	
                          {%- endif -%}	
                        </div>	        
                        
                      </div>
                      
						{% comment %}{% include 'cart-variant-upsells' %}{% endcomment %}
                    </div>
                    <div class="col-sm-2 col-md-2 col-xs-2  grid__item  remove-area mobile_remove_block">
                        <a href="{{ routes.cart_change_url }}?line={{ forloop.index }}&amp;quantity=0" class="cart__remove">
                            <svg width="15" height="16" viewBox="0 0 15 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                              <line x1="0.353553" y1="0.646447" x2="14.4957" y2="14.7886" stroke="black"/>
                              <line x1="0.414025" y1="14.7885" x2="14.5562" y2="0.6464" stroke="black"/>
                            </svg>
                        </a>
                      </div>
                  </div>

                      <div class="col-sm-6 col-md-6 col-xs-12 cart_row_Cart">
                      <div class="col-sm-4 col-md-4 col-xs-12 price-area">
                        <span class="cart__mini-labels">{{ 'cart.label.price' | t }}</span>
                        
                        {%- if item.original_price != item.final_price -%}
                        <span class="visually-hidden">{{ 'products.product.regular_price' | t }}</span>
                        <del class="h5">{{ item.original_price | money }}</del>
                        <span class="visually-hidden">{{ 'products.product.sale_price' | t }}</span>
                        <span class="order-discount order-discount--price h5">{{ item.final_price | money }}</span>
                        {%- else -%}
                        
                        <span class="h5">
                      {%if item.variant.compare_at_price > item.original_price %}
                      <div class="pricing_with_sale">
                          <span class="sale_pricing">
                      {{ item.original_price | money }}
                            <span class="compare_price">{{ item.variant.compare_at_price  | money }}</span> 
                      </span>
                      
                      {% capture saved_amount %}{{ item.variant.compare_at_price | minus: item.original_price | times: 100 | divided_by: item.variant.compare_at_price | round }}{% endcapture %}
                        {% capture saved_amounts %}{{ item.variant.compare_at_price | minus: item.original_price }}{% endcapture %}
                      
                          <div class="save_price">You save &nbsp; {{saved_amounts | money }} 
                        <span class="save_percentage">({{ saved_amount }}% off)</span>
                        </div>
                        
                      </div>
                      
                      {% else %}
                      {{ item.original_price | money }}
                      {% endif %}
                      
                  </span>
                        {%- endif -%}

                        {%- if item.variant.available and item.variant.unit_price_measurement -%}
                        {% include 'product-unit-price', variant: item, available: item.variant.available %}
                        {%- endif -%}

                        {%- if item.line_level_discount_allocations != blank -%}
                        <ul class="order-discount order-discount--list order-discount--title order-discount--cart medium-down--hide" aria-label="{{ 'customer.order.discount' | t }}">
                          {%- for discount_allocation in item.line_level_discount_allocations -%}
                          <li class="order-discount__item">
                            <span class="icon icon-saletag" aria-hidden="true"></span>{{ discount_allocation.discount_application.title }} (-{{ discount_allocation.amount | money }})
                          </li>
                          {%- endfor -%}
                        </ul>
                        {%- endif -%}
                      </div>

                      <div class="col-sm-3 col-md-3 col-xs-3 qty-area desktop-qty-block">
                        <label for="updates_{{ item.key }}" class="cart__mini-labels">{{ 'cart.label.quantity' | t }}</label>
                        <input type="number" class="cart__quantity-selector" name="updates[]" id="updates_{{ item.key }}" value="{{ item.quantity }}" min="0" aria-label="{{ 'cart.label.quantity' | t }}">
                      </div>

                      <div class="col-sm-3 col-md-3 col-xs-3 price-area">
                        <span class="cart__mini-labels">{{ 'cart.label.total' | t }}</span>
                        {%- if item.original_line_price != item.final_line_price -%}
                        <span class="visually-hidden">{{ 'products.product.regular_price' | t }}</span>
                        <del class="h5">{{ item.original_line_price | money }}</del>
                        <span class="visually-hidden">{{ 'products.product.sale_price' | t }}</span>
                        <span class="order-discount h5">{{ item.final_line_price | money }}</span>
                        {%- else -%}
                        <span class="h5">{{ item.original_line_price | money }}</span>
                        {%- endif -%}
                      </div>

                      <div class="grid__item text-center post-large--hide">
                        {%- if item.line_level_discount_allocations != blank -%}
                        <ul class="order-discount order-discount--list order-discount--title order-discount--cart" aria-label="{{ 'customer.order.discount' | t }}">
                          {%- for discount_allocation in item.line_level_discount_allocations -%}
                          <li class="order-discount__item">
                            <span class="icon icon-saletag" aria-hidden="true"></span>{{ discount_allocation.discount_application.title }} (-{{ discount_allocation.amount | money }})
                          </li>
                          {%- endfor -%}
                        </ul>
                        {%- endif -%}
                      </div>
                        <div class="col-sm-2 col-md-2 col-xs-2  grid__item  remove-area">
                          <a href="{{ routes.cart_change_url }}?line={{ forloop.index }}&amp;quantity=0" class="cart__remove">
                            <svg width="15" height="16" viewBox="0 0 15 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                              <line x1="0.353553" y1="0.646447" x2="14.4957" y2="14.7886" stroke="black"/>
                              <line x1="0.414025" y1="14.7885" x2="14.5562" y2="0.6464" stroke="black"/>
                            </svg>

                          </a>
                        </div>
                  </div>
  {% comment %}
                  Continue shopping button
                  {% endcomment %}
               
                </div>
                
              </div>
              {% endfor %}

              
            </div>
            <div class="col-md-5 col-sm-12 col-xs-12 desktop_payment">
               {% if section.settings.google_btn != blank or section.settings.facebook_btn != blank %}
              <div class="logos_outer">
              {% if section.settings.google_btn != blank %}
              <div class="google_btn">
                <img src="{{ section.settings.google_btn | img_url:'original' }}"/>
              </div>
              {% endif %}
              {% if section.settings.facebook_btn != blank %}
              <div class="facebook_btn">
                <img src="{{ section.settings.facebook_btn | img_url:'original' }}"/>
              </div>
              {% endif %}
              {% if section.settings.rating_btn != blank %}
              <div class="facebook_btn">
                <img src="{{ section.settings.rating_btn | img_url:'original' }}"/>
              </div>
              {% endif %}
              </div>
              {% endif %}
              <div class="cart_bottom">
              <div class="grid__right">
<!--                 <span class="h4">CART TOTALS</span> -->
                <span class="cart_free_shipping"><svg width="28" height="26" viewBox="0 0 28 26" fill="none" xmlns="http://www.w3.org/2000/svg">
