              const updatedDrawer = newHtml.querySelector('.cart_drawer_outer');
              if (updatedDrawer) {
                drawer.innerHTML = updatedDrawer.innerHTML;
              }
            }
          });
      }
    })
    .catch(err => {
      console.error('Add gift failed:', err);
    });
  }
