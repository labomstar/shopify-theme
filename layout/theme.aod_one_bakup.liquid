<script>
  document.addEventListener('DOMContentLoaded', function () {
    const TRAVEL_CASE_VARIANT_ID = 46003746046118;
    const DISCOUNT_CODE = 'FREETRAVEL';
    const THRESHOLD = 35000;

    function getCart(callback) {
      fetch('/cart.js')
        .then(res => res.json())
        .then(callback);
    }

    function updateCart() {
      getCart(cart => {
        const hasTravelCase = cart.items.some(item => item.variant_id === TRAVEL_CASE_VARIANT_ID);
        const totalPrice = cart.items.reduce((sum, item) => sum + item.line_price, 0);

        if (totalPrice >= THRESHOLD && hasTravelCase) {
          applyDiscount();
        } else {
          sessionStorage.removeItem('cartRefreshedOnce');
          removeDiscount();
        }
      });
    }

    function applyDiscount() {
      fetch('/discount/' + DISCOUNT_CODE)
        .then(() => {
          if (!sessionStorage.getItem('cartRefreshedOnce')) {
            sessionStorage.setItem('cartRefreshedOnce', 'true');
            location.reload();
          }
        });
    }

    function removeDiscount() {
      fetch('/cart/change.js', {
        method: 'POST',
        headers: { 'Content-Type': 'application/json' },
        body: JSON.stringify({ updates: {}, attributes: { discount_code: '' } })
      });
    }

    setTimeout(updateCart, 2000);
  });
</script>
